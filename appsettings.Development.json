{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "snipetrain": "mongodb://localhost:27017"
  },
  "Cors": {
    "allowedOrigins": [
      "http://localhost:4200",
      "http://127.0.0.1:8080"
    ]
  },
  "Endpoints": {
    "GameMe": "http://snipetrain.gameme.com/api/"
  },
  "HostedServices": {
    "delay": 10,
    "errorCountLimit": 10
  }
}
